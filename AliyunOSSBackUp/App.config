<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <add key="accessKeyId" value="accessKeyId" />
    <add key="accessKeySecret" value="accessKeySecret" />
    <add key="endpoint" value="http://oss-cn-hangzhou.aliyuncs.com" />
    <add key="bucketName" value="bucketName" />
    <!--备份文件路径，支持多路径 使用英文 ';'分隔-->
    <add key="backupFilePath" value="D:\DataBase\BackUp;D:\WebApp" />
    <!--OSS文件保存路径-->
    <add key="baseobjectName" value="DataBackUp/" />
    <!--文件最大保存版本数-->
    <add key="maxSizeRollBackups" value="10" />
  </appSettings>
</configuration>