<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <add key="accessKeyId" value="xxx" />
    <add key="accessKeySecret" value="xxxx" />
    <add key="endpoint" value="http://oss-cn-hangzhou.aliyuncs.com" />
    <add key="bucketName" value="xxxx" />
    <!--备份文件路径-->
    <add key="backupFilePath" value="C:\UserFile" />
    <!--OSS文件保存路径-->
    <add key="baseobjectName" value="DataBackUp/" />
    <!--文件保存天数-->
    <add key="maxDayBackups" value="30" />
  </appSettings>
</configuration>